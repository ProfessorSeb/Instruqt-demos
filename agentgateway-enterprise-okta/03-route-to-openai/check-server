#!/bin/bash
set -euxo pipefail
if ! kubectl get httproute openai -n enterprise-agentgateway &>/dev/null; then
  fail-message "HTTPRoute 'openai' not found in enterprise-agentgateway namespace."
fi
if ! kubectl get agentgatewaybackend openai-backend -n enterprise-agentgateway &>/dev/null; then
  fail-message "AgentgatewayBackend 'openai-backend' not found in enterprise-agentgateway namespace."
fi
